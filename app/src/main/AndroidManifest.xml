<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.noms.noms"
    android:versionCode="1.0"
    android:versionName="First Release">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:screenOrientation="portrait"
            android:name=".main_folder.login"
            android:theme="@style/MyCustomTheme.OverFlow">
        </activity>
        <activity
            android:name=".main_folder.register"
            android:label="@string/title_activity_register"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".forgot_password_folder.reset_password"
            android:label="@string/title_activity_reset_password"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".forgot_password_folder.change_password"
            android:label="@string/title_activity_change_password"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".friends_folder.friends"
            android:label="@string/title_activity_friends"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".groups_folder.group"
            android:label="@string/title_activity_group"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".settings_folder.setting"
            android:label="@string/title_activity_setting"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".friends_folder.add_friends"
            android:label="@string/title_activity_add_friends"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".groups_folder.create_group"
            android:label="@string/title_activity_create_group"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".groups_folder.AFTNG"
            android:label="@string/title_activity_add_friends_to_new_group"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".groups_folder.group_info"
            android:label="@string/title_activity_group_info"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".groups_folder.AFTG"
            android:label="@string/title_activity_add_friends_to_group"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".events_folder.events"
            android:label="@string/title_activity_events"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".events_folder.new_event"
            android:label="@string/title_activity_new_event"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".events_folder.invite_friends"
            android:label="@string/title_activity_invite_friends"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".user_info.user_info"
            android:label="@string/title_activity_user_info"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".events_folder.upcoming_events"
            android:label="@string/title_activity_upcoming_events"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".events_folder.event_details"
            android:label="@string/title_activity_event_details"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".events_folder.event_members"
            android:label="@string/title_activity_event_members"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
